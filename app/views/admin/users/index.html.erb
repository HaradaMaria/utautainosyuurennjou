  <h1>会員一覧</h1>
  <table class="table text-light text-center">
    <thead>
      <tr>
        <th class="col-1">ID</th>
        <th class="col-4">会員名</th>
        <th class="col-2">修練状況</th>
      </tr>
    </thead>
    
    <tbody>
      <% @users.each do |user|%>
      <tr>
        <td><%= user.id%></td>
        <td>
          <%= link_to admin_user_path(user.id) do %>
            <%= user.name %>
          <% end %>
          </td>
        <td><%= user.is_training ? '修練中' : '退会済'%></td>
      </tr>
      <% end%>
    </tbody>
  </table>