  <table>
    <thead>
      <th>会員名</th>
      <th>記録日時</th>
      <th>点数</th>
      <th>キー</th>
      <th></th>
    </thead>
    <tbody>
      <% records.each do |record| %>
        <tr>
          <td><%= record.name %></td>
          <td><%= record.created_at %></td>
          <td><%= record.score %></td>
          <td><%= record.key %></td>
        </tr>
      <% end %>
    </tbody>
    
  </table>